config globals 'globals'
	option ip_default 'pass'
	option mac_default 'pass'
	option url_default 'pass'
	option ip_enable '1'
	option mac_enable '1'
	option url_enable '1'

config rule
	option type 'ip'
	option time '0000-2300'
	option src  '192.168.1.2-192.168.1.254'
	option dst  '8.8.8.8-8.8.8.8'
	option dport '53'
#	option sport '80-81'
	option proto 'udp'
	option pass '0'
	option enabled '1'

config rule
	option type 'mac'
	option time '0000-2359'
	option mac 'e0:06:e6:c3:1c:10'
	option enabled '1'

config rule
	option type 'url'
	option time '0000-2359'
	option url 'sina.com.cn'
	option enabled '1'
